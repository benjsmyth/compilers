int foo(int x, int y) {
  int a[10];
  int i = 0;
  while (i < 10) {
    a[i] = 0;
    i = i + 1;
  }
  while (x < 10) {
    a[x] = y;
    x = x + 1;
  }
  int c = 0;
  i = 0;
  while (i < 10) {
    c = c + a[i];
  }
  return c;
}

int main(void) {
  int c;
  c = foo(5, 1);
  return c;
}
